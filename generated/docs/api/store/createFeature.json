                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             \n      </td>\n    </tr>\n  </tbody>\n</table>\n\n\n<h4 class=\"no-anchor\" id=\"returns-3\">Returns</h4>\n<p><code>Feature&#x3C;AppState, FeatureName, FeatureState></code></p>\n\n\n\n\n\n\n\n    </td>\n  </tr>\n  \n  </tbody></table>\n</section>\n\n\n  \n<section class=\"usage-notes\">\n  <h2 id=\"usage-notes\">Usage notes<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/store/createFeature#usage-notes\"><i class=\"material-icons\">link</i></a></h2>\n  <code-example language=\"ts\">\ninterface ProductsState {\n  products: Product[];\n  selectedId: string | null;\n}\n\nconst initialState: ProductsState = {\n  products: [],\n  selectedId: null,\n};\n\nconst productsFeature = <a href=\"api/store/createFeature\" class=\"code-anchor\">createFeature</a>({\n  name: 'products',\n  reducer: <a href=\"api/store/createReducer\" class=\"code-anchor\">createReducer</a>(\n    initialState,\n    <a href=\"api/store/on\" class=\"code-anchor\">on</a>(ProductsApiActions.loadSuccess(<a href=\"api/store-devtools/StoreDevtools#state\" class=\"code-anchor\">state</a>, { products }) => ({\n      ...state,\n      products,\n    }),\n  ),\n});\n\nconst {\n  name,\n  reducer,\n  // feature <a href=\"api/store/testing/MockSelector#selector\" class=\"code-anchor\">selector</a>\n  selectProductsState, // type: <a href=\"api/store/MemoizedSelector\" class=\"code-anchor\">MemoizedSelector</a>&#x3C;Record&#x3C;string, any>, ProductsState>\n  // feature <a href=\"api/store-devtools/StoreDevtools#state\" class=\"code-anchor\">state</a> properties selectors\n  selectProducts, // type: <a href=\"api/store/MemoizedSelector\" class=\"code-anchor\">MemoizedSelector</a>&#x3C;Record&#x3C;string, any>, Product[]>\n  selectSelectedId, // type: <a href=\"api/store/MemoizedSelector\" class=\"code-anchor\">MemoizedSelector</a>&#x3C;Record&#x3C;string, any>, string | null>\n} = productsFeature;\n</code-example>\n<p><strong>Creating Feature with Extra Selectors</strong></p>\n<code-example language=\"ts\">\ntype CallState = 'init' | '<a href=\"api/data/EntityCollection#loading\" class=\"code-anchor\">loading</a>' | '<a href=\"api/data/EntityCollection#loaded\" class=\"code-anchor\">loaded</a>' | { error: string };\n\ninterface <a href=\"api/store/State\" class=\"code-anchor\">State</a> extends <a href=\"api/entity/EntityState\" class=\"code-anchor\">EntityState</a>&#x3C;Product> {\n  callState: CallState;\n}\n\nconst <a href=\"api/data/EntityCollectionReducerMethods#adapter\" class=\"code-anchor\">adapter</a> = <a href=\"api/entity/createEntityAdapter\" class=\"code-anchor\">createEntityAdapter</a>&#x3C;Product>();\nconst initialState: <a href=\"api/store/State\" class=\"code-anchor\">State</a> = adapter.getInitialState({\n  callState: 'init',\n});\n\n<a href=\"api/store-devtools/DevToolsFeatureOptions#export\" class=\"code-anchor\">export</a> const productsFeature = <a href=\"api/store/createFeature\" class=\"code-anchor\">createFeature</a>({\n  name: 'products',\n  reducer: <a href=\"api/store/createReducer\" class=\"code-anchor\">createReducer</a>(initialState),\n  extraSelectors: ({ selectProductsState, selectCallState }) => ({\n    ...adapter.getSelectors(selectBooksState),\n    ...getCallStateSelectors(selectCallState)\n  }),\n});\n\nconst {\n  name,\n  reducer,\n  // feature <a href=\"api/store/testing/MockSelector#selector\" class=\"code-anchor\">selector</a>\n  selectProductsState,\n  // feature <a href=\"api/store-devtools/StoreDevtools#state\" class=\"code-anchor\">state</a> properties selectors\n  selectIds,\n  selectEntities,\n  selectCallState,\n  // selectors returned by `adapter.getSelectors`\n  selectAll,\n  selectTotal,\n  // selectors returned by `getCallStateSelectors`\n  selectIsLoading,\n  selectIsLoaded,\n  selectError,\n} = productsFeature;\n</code-example>\n\n</section>\n\n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/store\n - enterprise-support\n - guide/migration/v15\n - guide/store/feature-creators\n-->\n<!-- links from this doc:\n - /api\n - api/data/EntityCollection#loaded\n - api/data/EntityCollection#loading\n - api/data/EntityCollectionReducerMethods#adapter\n - api/entity/EntityState\n - api/entity/createEntityAdapter\n - api/store\n - api/store-devtools/DevToolsFeatureOptions#export\n - api/store-devtools/StoreDevtools#state\n - api/store/FeatureConfig\n - api/store/MemoizedSelector\n - api/store/State\n - api/store/createFeature#createfeature\n - api/store/createFeature#overloads\n - api/store/createFeature#usage-notes\n - api/store/createReducer\n - api/store/on\n - api/store/testing/MockSelector#selector\n - https://github.com/ngrx/platform/edit/master/modules/store/src/feature_creator.ts?message=docs(store)%3A%20describe%20your%20change...#L106-L232\n - https://github.com/ngrx/platform/issues/3737\n - https://github.com/ngrx/platform/tree/15.1.0/modules/store/src/feature_creator.ts#L106-L232\n-->"
