                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        \n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               \n      </td>\n    </tr>\n  </tbody>\n</table>\n  \n  \n</section>\n\n\n  \n\n  \n\n\n  \n<section class=\"usage-notes\">\n  <h2 id=\"usage-notes\">Usage notes<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/component/PushPipe#usage-notes\"><i class=\"material-icons\">link</i></a></h2>\n  <code-example language=\"html\">\n&#x3C;p>{{ number$ | <a href=\"api/component/PushPipe\" class=\"code-anchor\">ngrxPush</a> }}&#x3C;/p>\n\n&#x3C;ng-container *ngIf=\"number$ | <a href=\"api/component/PushPipe\" class=\"code-anchor\">ngrxPush</a> as n\">{{ n }}&#x3C;/ng-container>\n\n&#x3C;app-number [number]=\"number$ | <a href=\"api/component/PushPipe\" class=\"code-anchor\">ngrxPush</a>\">&#x3C;/app-number>\n</code-example>\n\n</section>\n\n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/component\n - api/component/PushModule\n - guide/component/let\n - guide/component/push\n - guide/migration/v13\n - guide/migration/v14\n-->\n<!-- links from this doc:\n - /api\n - api/component\n - api/component/PushModule\n - api/component/PushPipe#input-value\n - api/component/PushPipe#ngmodule\n - api/component/PushPipe#pushpipe\n - api/component/PushPipe#usage-notes\n - https://github.com/ngrx/platform/edit/master/modules/component/src/push/push.pipe.ts?message=docs(component)%3A%20describe%20your%20change...#L17-L88\n - https://github.com/ngrx/platform/tree/14.0.0-beta.0/modules/component/src/push/push.pipe.ts#L17-L88\n-->"
